{
  "name": "@srclaunch/web-environment",
  "description": "Web environment specific utilities for SrcLaunch applications",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "ISC",
  "version": "0.1.8",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "build": "yarn clean && yarn build:applab",
    "build:applab": "yarn applab build",
    "clean": "rm -fr ./dist",
    "git:push": "git push --follow-tags origin main",
    "qr": "yarn build && git add . && git commit -m \"Quick release - patch\" && yarn release",
    "release": "yarn version:patch && yarn git:push",
    "test": "ava",
    "version:patch": "standard-version",
    "yui": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@srclaunch/cli": "^0.1.92",
    "@srclaunch/dx": "^0.1.18",
    "@srclaunch/types": "^0.1.28",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "ava": "^4.1.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-testing-library": "^5.2.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.6.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
