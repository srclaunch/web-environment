{
  "name": "@srclaunch/cli",
  "description": "Command line tools used by SrcLaunch applications and services",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "0.1.267",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "srclaunch": "dist/index.mjs"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build:es": "esbuild --external:c8 --external:esbuild  --external:'esbuild-css-modules-plugin' --external:'fs-extra' --external:ink --external:jest  --external:'jest-cli' --external:'jest-pnp-resolver' --external:meow --external:'@rauschma/stringio' --external:'simple-git' --external:'standard-version'  --external:typescript --external:vite --external:'@vitejs/plugin-react' --external:'update-notifier' --sourcemap --bundle --minify --outfile=dist/index.js --format=esm --target=esnext --define:'navigator={}' --platform=node src/index.ts",
    "build:self": "node ./dist/index.js build",
    "build": "yarn build:es && yarn build:self",
    "dev": "NODE_ENV=dev && node --loader ts-node/esm --experimental-specifier-resolution=node ./src/index.ts",
    "git:push": "git push --follow-tags origin main",
    "qr": "yarn build && git add . && git commit -m \"Quick release - patch\" && yarn release",
    "release": "yarn version:patch && yarn git:push",
    "start": "node --experimental-modules --experimental-specifier-resolution=node dist/index.js",
    "test": "yarn ava ./src/tests/**",
    "test:coverage": "yarn c8 --all --clean --include src --exclude 'src/tests/**' ava ./src/tests/** && yarn c8 report --all --reporter json-summary --reporter lcov --include src --exclude 'src/tests/**'",
    "test:watch": "yarn ava --watch",
    "version:patch": "standard-version",
    "yui": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@rauschma/stringio": "^1.4.0",
    "@srclaunch/exceptions": "^0.1.48",
    "@vitejs/plugin-react": "^1.3.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "ava": "^4.2.0",
    "c8": "^7.11.2",
    "chalk": "^5.0.1",
    "change-case": "^4.1.2",
    "css-tree": "^2.1.0",
    "esbuild": "^0.14.38",
    "fs-extra": "^10.1.0",
    "ink": "^3.2.0",
    "ink-multi-select": "^2.0.0",
    "ink-spinner": "^4.0.3",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "luxon": "^2.3.2",
    "meow": "^10.1.2",
    "nanoid": "^3.3.3",
    "pluralize": "^8.0.0",
    "react": "^18.0.0",
    "shelljs": "^0.8.5",
    "simple-git": "^3.7.1",
    "terminal-link": "^3.0.0",
    "typescript": "^4.6.3",
    "update-notifier": "^5.1.0",
    "vite": "^2.9.5"
  },
  "devDependencies": {
    "@srclaunch/dx": "^0.1.43",
    "@srclaunch/types": "^0.1.94",
    "@types/fs-extra": "^9.0.13",
    "@types/jest-cli": "^24.3.0",
    "@types/luxon": "^2.3.1",
    "@types/meow": "^6.0.0",
    "@types/node": "^17.0.25",
    "@types/node-emoji": "^1.8.1",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^18.0.6",
    "@types/rimraf": "^3.0.2",
    "@types/standard-version": "^7.0.1",
    "@types/update-notifier": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-regexp": "^1.7.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-testing-library": "^5.3.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "stylelint": "^14.7.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0"
  },
  "resolve": {
    "alias": {
      "react/jsx-dev-runtime": "react/jsx-dev-runtime.js",
      "react/jsx-runtime": "react/jsx-runtime.js"
    }
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "failFast": true,
    "nodeArguments": [
      "--loader=ts-node/esm"
    ],
    "verbose": true
  },
  "packageManager": "yarn@3.2.0"
}