{
  "name": "@srclaunch/types",
  "description": "Base Typescript definitions required by AppLab applications and services",
  "author": "Steven Bennett <steven@srclaunch.com",
  "license": "ISC",
  "version": "0.1.21",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "build:types": "tsc --build --force",
    "build:applab": "yarn applab build",
    "build": "yarn clean && yarn build:applab && yarn build:types",
    "clean": "rm -fr ./dist",
    "lint:eslint": "yarn eslint ./src --ext js,ts,tsx --max-warnings 0",
    "lint:stylelint": "yarn stylelint '**/*.ts[x]'",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "push": "git push --follow-tags origin main",
    "release": "yarn standard-version && git push --follow-tags origin main",
    "version:push": "git push --follow-tags origin main",
    "version:bump": "yarn version patch --force",
    "version:release": "yarn standard-version",
    "yui": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@srclaunch/cli": "^0.1.18",
    "@srclaunch/dx": "^0.1.15",
    "@types/async-exit-hook": "~2.0.0",
    "@types/axios": "^0.14.0",
    "@types/express": "~4.17.13",
    "@types/luxon": "^2.0.9",
    "@types/node": "17.0.15",
    "@types/react": "^17.0.39",
    "@types/serialize-error": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "eslint-plugin-unicorn": "^40.1.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "serialize-error": "^9.0.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.3.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  },
  "packageManager": "yarn@3.2.0"
}