{
  "name": "@srclaunch/exceptions",
  "description": "Exception handling utilities",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "ISC",
  "version": "0.1.48",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "build": "yarn srclaunch build",
    "git:push": "git push --follow-tags origin main",
    "qr": "yarn build && git add . && git commit -m \"Quick release - patch\" && yarn release",
    "release": "yarn test && yarn version:patch && yarn git:push",
    "test": "yarn ava",
    "test:coverage": "yarn c8 ava --all --clean --include src && yarn c8 report --all  --reporter json-summary --include src",
    "test:watch": "yarn ava --watch",
    "version:patch": "standard-version",
    "yui": "yarn upgrade-interactive"
  },
  "dependencies": {
    "luxon": "^2.3.2",
    "nanoid": "^3.3.3",
    "serialize-error": "^9.1.1"
  },
  "devDependencies": {
    "@srclaunch/cli": "^0.1.179",
    "@srclaunch/dx": "^0.1.19",
    "@srclaunch/types": "^0.1.82",
    "@types/express": "^4.17.13",
    "@types/luxon": "^2.3.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "ava": "^4.2.0",
    "c8": "^7.11.2",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-regexp": "^1.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-testing-library": "^5.3.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.7.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "packageManager": "yarn@3.2.0"
}