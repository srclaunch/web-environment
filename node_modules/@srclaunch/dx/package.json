{
  "name": "@srclaunch/dx",
  "description": "Linter config and other developer experience related configs",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "0.1.15",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "files": [
    ".commitlintrc.js",
    ".commitlintrc.cjs",
    ".eslintrc.js",
    ".eslintrc.cjs",
    ".eslintrc.ui.js",
    ".eslintrc.ui.cjs",
    ".prettierrc.js",
    ".prettierrc.cjs",
    ".stylelintrc.js",
    ".stylelintrc.cjs",
    ".stylelintrc.ui.js",
    ".stylelintrc.ui.cjs",
    ".versionrc.js",
    ".versionrc.cjs",
    "ava.config.js",
    "ava.config.cjs",
    "jest.config.js",
    "jest.config.cjs",
    "tsconfig.json",
    "tsconfig.ui.json"
  ],
  "exports": {
    "./.commitlintrc": {
      "import": "./.commitlintrc.js",
      "require": "./.commitlintrc.cjs"
    },
    "./.eslintrc": {
      "import": "./.eslintrc.js",
      "require": "./.eslintrc.cjs"
    },
    "./.eslintrc.ui": {
      "import": "./.eslintrc.ui.js",
      "require": "./.eslintrc.ui.cjs"
    },
    "./.prettierrc": {
      "import": "./.prettierrc.js",
      "require": "./.prettierrc.cjs"
    },
    "./.stylelintrc": {
      "import": "./.stylelintrc.js",
      "require": "./.stylelintrc.cjs"
    },
    "./.stylelintrc.ui": {
      "import": "./.stylelintrc.ui.js",
      "require": "./.stylelintrc.ui.cjs"
    },
    "./.versionrc": {
      "import": "./.versionrc.js",
      "require": "./.versionrc.cjs"
    },
    "./ava.config": {
      "import": "./ava.config.js",
      "require": "./ava.config.cjs"
    },
    "./jest.config": {
      "import": "./jest.config.js",
      "require": "./jest.config.cjs"
    },
    "./tsconfig.json": {
      "import": "./tsconfig.json",
      "require": "./tsconfig.json"
    },
    "./tsconfig.ui.json": {
      "import": "./tsconfig.ui.json",
      "require": "./tsconfig.ui.json"
    }
  },
  "scripts": {
    "push": "git push --follow-tags origin main",
    "release": "standard-version && git push --follow-tags origin main",
    "yui": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "standard-version": "^9.3.2"
  },
  "packageManager": "yarn@3.2.0"
}